          gmp_cv_prog_host_cc=$c
          break
        fi
      fi
    done
    rm -f conftest*
    if test -z "$gmp_cv_prog_host_cc"; then
      { { echo "$as_me:5779: error: cannot find a build system compiler" >&5
echo "$as_me: error: cannot find a build system compiler" >&2;}
   { (exit 1); exit 1; }; }
    fi
  fi
fi

